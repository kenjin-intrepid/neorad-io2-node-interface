{
  "name": "neoRAD-IO2",
  "productName": "neoRAD-IO2",
  "version": "1.6.18preview2",
  "description": "neoRAD-IO2 GUI app",
  "main": "main.js",
  "scripts": {
    "install": "cmake-js compile",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "",
  "keywords": [
    "neoRAD-IO2",
    "IntrepidCS"
  ],
  "author": "IntrepidCS",
  "license": "MIT",
  "devDependencies": {
    "electron": "^6.1.12",
    "electron-builder": "^21.2.0",
    "electron-notarize": "^0.3.0"
  },
  "versions": {
    "electron": "^6.1.12",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bindings": "^1.5.0",
    "bootstrap": "^4.5.0",
    "electron-is-dev": "^1.2.0",
    "electron-settings": "^3.2.0",
    "electron-window-state": "^5.0.3",
    "jquery": "^3.5.1",
    "nan": "^2.14.1",
    "papaparse": "^5.2.0",
    "popper.js": "^1.16.0",
    "sudo-prompt": "^9.2.1"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "6.1.9",
    "arch": "x64"
  },
  "build": {
    "appId": "com.intrepidcs.neoRADIO2",
    "directories": {
      "buildResources": "build/release"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "win": {
      "icon": "lib/img/logo.ico"
    },
    "mac": {
      "icon": "lib/img/logo.icns"
    },
    "linux": {
      "icon": "lib/img/logo.icns"
    },
    "copyright": "Copyright Â© 2020 ${author}"
  }
}
