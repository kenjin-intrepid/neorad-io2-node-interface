{
  "name": "neoRAD-IO2",
  "productName": "neoRAD-IO2",
  "version": "1.4.14",
  "description": "neoRAD-IO2 GUI app",
  "main": "main.js",
  "scripts": {
    "install": "cmake-js compile",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "",
  "keywords": [
    "neoRAD-IO2",
    "IntrepidCS"
  ],
  "author": "IntrepidCS",
  "license": "MIT",
  "devDependencies": {
    "electron": "^4.1.4",
    "electron-builder": "^20.40.2"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "bindings": "^1.5.0",
    "bootstrap": "^4.3.1",
    "electron-settings": "^3.2.0",
    "electron-window-state": "^5.0.3",
    "emit-stream": "^0.1.2",
    "jquery": "^3.4.0",
    "nan": "^2.13.2",
    "papaparse": "^4.6.3",
    "popper.js": "^1.15.0",
    "through": "^2.3.8"
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "4.1.4",
    "arch": "x64"
  },
  "build": {
    "appId": "com.intrepidcs.neoRADIO2",
    "directories": {
      "buildResources": "build/release"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "copyright": "Copyright Â© 2019 ${author}"
  }
}
